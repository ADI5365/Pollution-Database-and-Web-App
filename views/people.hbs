<html>
<header>
    <h1>People</h1>
</header>

{{!-- Nav bar for links to all pages on the site --}}
<nav>
    <a href="/">Home</a>
    <a href="/locations">Locations</a>
    <a href="/people">People</a>
    <a href="/health-problems">Health Problems</a>
    <a href="/individual-health-issues">Individual Health Issues</a>
    <a href="/city-health-issues">Health Issues By City</a>
    <a href="/pollution-by-day">Dates with Pollution Data</a>
    <a href="/daily-location-pollution">Daily Pollution in Each Location</a>
</nav>

{{!-- Locations Display Table --}}
<div id="browse">
    <h3>Browse People</h3>

    <table id="people-table">
        <thead>
    
            <tr>
                {{#each data.[0]}}
                <th>
                    {{@key}}
                </th>
                {{/each}}
            </tr>
        </thead>
    
        <tbody>
    
            {{#each data}}
            <tr>
                <td>{{this.person_ID}}</td>
                <td>{{this.age}}</td>
                <td>{{this.location_ID}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
</html>